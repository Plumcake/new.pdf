         var results = [];
         var fld = this.getField("secretField");
         if (fld && fld.value) {
            results.push("Secret value stolen: " + fld.value);
         } else {
            results.push("No field or empty");
         }
         if (this.info && this.info.Title) {
            results.push("Doc info Title: " + this.info.Title);
         } else {
            results.push("No doc info Title");
         }
         app.alert(results.join("\\n") || "Nothing readable found");
      } catch(e) {
         app.alert("Error during probe: " + e);
